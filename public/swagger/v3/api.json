        "/api/v3/grid": {
            "post": {
                "tags": [
                    "Grid"
                ],
                "summary": "Create a fishnet grid from an input polygon",
                "operationId": "f021edc77ef1948114805eaa7466f88f",
                "requestBody": {
                    "description": "Fishnet parameters",
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "table": {
                                        "type": "string",
                                        "example": "new_grid"
                                    },
                                    "extent": {
                                        "type": "string",
                                        "example": "my_extent_polygon"
                                    },
                                    "size": {
                                        "type": "integer",
                                        "example": 10000
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Return true if fishnet grid was created",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/v3/oauth/token": {
            "post": {
                "tags": [
                    "OAuth"
                ],
                "summary": "Get token",
                "operationId": "bc3f606752bcc17301bf34221226bd9c",
                "requestBody": {
                    "description": "OAuth password grant parameters",
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "grant_type": {
                                        "type": "string",
                                        "example": "password"
                                    },
                                    "username": {
                                        "type": "string",
                                        "example": "user@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "1234Luggage"
                                    },
                                    "database": {
                                        "type": "string",
                                        "example": "roads"
                                    },
                                    "client_id": {
                                        "type": "string",
                                        "example": "xxxxxxxxxx"
                                    },
                                    "client_secret": {
                                        "type": "string",
                                        "example": "xxxxxxxxxx"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Operation status",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "access_token": {
                                            "type": "string",
                                            "example": "MTQ0NjJkZmQ5OTM2NDE1ZTZjNGZmZjI3"
                                        },
                                        "token_type": {
                                            "type": "string",
                                            "example": "bearer"
                                        },
                                        "expires_in": {
                                            "type": "integer",
                                            "example": 3600
                                        },
                                        "refresh_token": {
                                            "type": "string",
                                            "example": "IwOGYzYTlmM2YxOTQ5MGE3YmNmMDFkNTVk"
                                        },
                                        "scope": {
                                            "type": "string",
                                            "example": "sql"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
