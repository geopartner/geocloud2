{
    "openapi": "3.0.0",
    "info": {
        "title": "GC2 API",
        "contact": {
            "email": "mh@mapcentia.com"
        },
        "version": "1.0.0"
    },
    "paths": {
        "/api/v3/scheduler/{jobid}": {
            "post": {
                "tags": [
                    "Scheduler"
                ],
                "operationId": "startSchedulerJob",
                "parameters": [
                    {
                        "name": "jobid",
                        "in": "path",
                        "description": "Job id",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "876"
                    }
                ],
                "responses": {
                    "204": {
                        "description": "No content"
                    }
                }
            }
        },
        "/api/v3/scheduler": {
            "get": {
                "tags": [
                    "Scheduler"
                ],
                "operationId": "getRunningSchedulerJobs",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "uuid": {
                                            "description": "Job uuid",
                                            "type": "string",
                                            "example": "my_index"
                                        },
                                        "id": {
                                            "description": "Job id",
                                            "type": "string",
                                            "example": "my_index"
                                        },
                                        "pid": {
                                            "description": "Job pid",
                                            "type": "string",
                                            "example": "my_index"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "name": "bearerAuth",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    }
}