        "/api/v3/sql": {
            "post": {
                "tags": [
                    "Sql"
                ],
                "summary": "Do SQL quyeries",
                "operationId": "bf38dfdb1ba98b813bd713b4992f82c3",
                "requestBody": {
                    "description": "Parameters",
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "q": {
                                        "type": "string",
                                        "example": "SELECT 1 as id,ST_setsrid(ST_MakePoint(10,56),4326) as geom"
                                    },
                                    "srs": {
                                        "description": "",
                                        "type": "integer",
                                        "example": "25832"
                                    },
                                    "format": {
                                        "type": "string",
                                        "example": "csv"
                                    },
                                    "geoformat": {
                                        "type": "string",
                                        "example": "wkt"
                                    },
                                    "allstr": {
                                        "type": "boolean",
                                        "example": false
                                    },
                                    "lifetime": {
                                        "type": "integer",
                                        "example": 0
                                    },
                                    "base64": {
                                        "type": "boolean",
                                        "example": false
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "If select then the result will be data on choosen format. If transaction the number of effected rows is returned.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/v3/tileseeder": {
            "get": {
                "tags": [
                    "Tileseeder"
                ],
                "summary": "Get all running mapcache_seed processes started by user",
                "operationId": "58b57136e60a53a96f4b373d357a5ffe",
                "responses": {
                    "200": {
                        "description": "Operation status"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Tileseeder"
                ],
                "summary": "Starts a mapcache_seed process",
                "operationId": "213cca73dcb037fddfe7c7bb29765407",
                "requestBody": {
                    "description": "mapcache_seed parameters",
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "My seeder job"
                                    },
                                    "layer": {
                                        "type": "string",
                                        "example": "my_schema.my_table"
                                    },
                                    "start": {
                                        "type": "integer",
                                        "example": 10
                                    },
                                    "end": {
                                        "type": "integer",
                                        "example": 10
                                    },
                                    "extent": {
                                        "type": "string",
                                        "example": "my_schema.my_table_with_extent"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Return the UUID and process id",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "uuid": {
                                            "type": "string",
                                            "example": "C4A3797E-EC6B-4DAC-9474-ADA9083620F3"
                                        },
                                        "pid": {
                                            "type": "integer",
                                            "example": 20326
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
